#include <iostream>

using namespace std;

int main()
{
    int tc;
    cin>>tc;
    for(int j=0;j<tc;j++)
    {
        int i,n;
        cin>>n;
        int arr[n];
        for(i=0;i<n;i++)
            cin>>arr[i];
        int key;
        int s=0,e=n-1,mid,ub=-1,lb=-1;
        cin>>key;
        while(s<=e)
        {
            mid=(s+e)/2;
            if(arr[mid]==key)
            {
                lb=mid;
                e=mid-1;
            }
            else if(key<arr[mid])
                e=mid-1;
            else
                s=mid+1;
        }
        s=0;
        e=n-1;
        while(s<=e)
        {
            mid=(s+e)/2;
            if(arr[mid]==key)
            {
                ub=mid;
                s=mid+1;
            }
            else if(key<arr[mid])
                e=mid-1;
            else
                s=mid+1;
        }
        if(ub==-1 && lb==-1)
            cout<<"Key Not Present";
        else
            cout<<key<<"-"<<ub-lb+1;
    return 0;
}
